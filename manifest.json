{
	"manifest_version": 2,
	"name": "Improved Notifications",
	"version": "0.2",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"content_scripts": [
		{
			"matches": [
				"https://github.com/notifications*",
				"https://github.com/*/notifications*"
			],
			"js": ["jquery-2.1.4.min.js", "content.js"],
			"css": ["content.css"]
		}
	]
}